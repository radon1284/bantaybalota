<%= form_for @report, :html => { :class => 'form-horizontal form-model' } do |f| %>
<% if @report.errors.any? %>
    <div id="error_explanation" class="alert alert-danger alert-dismissible" role="alert">
  <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
      <h4><%= pluralize(@report.errors.count, "error") %> prohibited this report from being saved:</h4>

      <ul>
      <% @report.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <%= f.label :precinct_number, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :precinct_number, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :ballot_number, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :ballot_number, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :vote_receipt_number, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :vote_receipt_number, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :bei_name, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :bei_name, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :voted_president, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
    <%= f.select :voted_president, Report.presidents.keys.map { |w| [w.titleize, w] }, {}, { :class => 'selectpicker' } %>
      
    </div>
  </div>
  <div class="form-group">
    <%= f.label :pcos_result_president, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
    <%= f.select :pcos_result_president, Report.presidents.keys.map { |w| [w.titleize, w] }, {}, { :class => 'selectpicker' } %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :voted_vice_president, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
    <%= f.select :voted_vice_president, Report.vice_presidents.keys.map { |w| [w.titleize, w] }, {}, { :class => 'selectpicker' } %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :pcos_result_vice_president, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
    <%= f.select :pcos_result_vice_president, Report.vice_presidents.keys.map { |w| [w.titleize, w] }, {}, { :class => 'selectpicker' } %>
    </div>
  </div>

  <div class="form-group">
    <div class='col-md-offset-2 col-md-10'>
      <%= f.submit nil, :class => 'btn btn-primary' %>
      <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                  reports_path, :class => 'btn btn-default' %>
    </div>
  </div>
<% end %>
