<%= form_for @report, :html => { :class => 'form-horizontal form-model' } do |f| %>
  <div class="form-group">
    <%= f.label :precinct_number, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :precinct_number, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :ballot_number, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :ballot_number, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :vote_receipt_number, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :vote_receipt_number, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :bei_name, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :bei_name, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :voted_president, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.number_field :voted_president, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :pcos_result_president, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.number_field :pcos_result_president, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :voted_vice_president, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.number_field :voted_vice_president, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :pcos_result_vice_president, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.number_field :pcos_result_vice_president, :class => 'text-field form-control' %>
    </div>
  </div>

  <div class="form-group">
    <div class='col-md-offset-2 col-md-10'>
      <%= f.submit nil, :class => 'btn btn-primary' %>
      <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                  reports_path, :class => 'btn btn-default' %>
    </div>
  </div>
<% end %>
